{
  "name": "desering-org-scripts",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run build:js && npm run build:jekyll",
    "build:jekyll": "bundle exec jekyll build",
    "build:js": "webpack build --mode production",
    "dev": "concurrently \"webpack watch --mode development\" \"JEKYLL_ENV=watch bundle exec jekyll serve\"",
    "get-sheets-data": "node _scripts/get-sheets-data.js",
    "get-playlists": "node _scripts/get-playlists.js",
    "serve": "bundle exec jekyll serve"
  },
  "dependencies": {
    "date-fns": "^2.28.0",
    "dialog-polyfill": "^0.5.6",
    "event-target-shim": "^6.0.2",
    "googleapis": "^100.0.0",
    "spotify-web-api-node": "^5.0.2",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "babel-loader": "^8.2.5",
    "concurrently": "^6.2.0",
    "core-js": "^3.22.5",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  }
}
