{
  "name": "desering-org-scripts",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run build:js && npm run build:jekyll",
    "build:jekyll": "JEKYLL_ENV=production bundle exec jekyll build",
    "build:js": "webpack build --mode production",
    "dev": "concurrently \"webpack watch --mode development\" \"npm run serve\"",
    "get-sheets-data": "node _scripts/get-sheets-data.js",
    "serve": "JEKYLL_ENV=development bundle exec jekyll serve"
  },
  "dependencies": {
    "date-fns": "^2.23.0",
    "googleapis": "^82.0.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.0",
    "core-js": "^3.16.0",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.7.2"
  }
}
